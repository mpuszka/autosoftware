{% extends 'base.html.twig' %}

{% block body %}
  <div class="col-md-8">
    <div id="article" data-id={{ id }}>
      <dynamicArticle/>
    </div>
  </div>

  <div class="col-md-4">
    <div id="articles" data-title="All articles" data-skip={{ id }}>
     <articles />
    </div>
  </div>
{% endblock %}

{% block addition_scripts %}
  {{ encore_entry_script_tags('article') }}
  {{ encore_entry_script_tags('articles') }}
{% endblock %}
